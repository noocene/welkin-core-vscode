{
    "name": "welkin-core",
    "displayName": "welkin-core",
    "description": "Language support for welkin-core",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.54.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "welkincore",
            "aliases": ["welkin-core", "welkincore"],
            "extensions": [".wc"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "welkincore",
            "scopeName": "source.welkincore",
            "path": "./syntaxes/welkincore.tmLanguage.json"
        }]
    }
}